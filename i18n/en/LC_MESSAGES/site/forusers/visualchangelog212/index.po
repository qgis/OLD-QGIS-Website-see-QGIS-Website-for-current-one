msgid "Sample URL:"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:898
msgid "``http://yourserver.org/cgi-bin/qgismapserver.fcgi?``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:899
msgid "``map=/path/to/your/projectfile.qgs&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:900
msgid "``SERVICE=WMS&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:901
msgid "``VERSION=1.3.0&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:902
msgid "``REQUEST=GetMAP&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:903
msgid "``FORMAT=application/dxf&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:904
msgid "``FORMAT_OPTIONS=SCALE:500;MODE:SYMBOLLAYERSYMBOLOGY&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:905
msgid "``FILE_NAME=youroutputfilename.dxf&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:906
msgid "``CRS=EPSG:EPSG:21781&``"
msgstr ""

#: ../../source/site/forusers/visualchangelog212/index.rst:907
